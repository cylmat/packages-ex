{
    "name": "cyril/packages",
    "description": "Essai sur differents packages, micro-frameworks et extensions php",
    "minimum-stability": "stable",
    "type": "project",
    "authors": [
        {
            "name": "freeonisland",
            "email": "freeonisland@yahoo.fr"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "bcosca/fatfree": "^3.7",
        "laravel/lumen-framework": "^7.0",
        "league/oauth2-client": "^2",
        "league/oauth2-server": "^8",
        "mikecao/flight": "^1",
        "slim/slim": "^4.4"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "friendsofphp/php-cs-fixer": "^2",
        "fzaninotto/faker": "^1",
        "kint-php/kint": "^3",
        "mockery/mockery": "^1",
        "phing/phing": "^2",
        "phpmd/phpmd": "^2.8",
        "phpspec/phpspec": "^6.0",
        "phpunit/phpunit": "^8.5",
        "slevomat/coding-standard": "^5.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload" : {
        "psr-4": {
            "App\\": "src/"
